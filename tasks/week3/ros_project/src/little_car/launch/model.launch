<launch>
	<arg name="model"/>
	<arg name="gui" default="False"/>
	<param name="little car" textfile="$(find little_car)/urdf/little_car.urdf"/>
	<param name="use_gui" value="$(arg gui)"/>
	<node name="little_car_node" pkg="little_car" type="little_car_node" output="screen"/>
    <node name="make_turn" pkg="little_car" type="make_turn" output="screen"/>
</launch>
